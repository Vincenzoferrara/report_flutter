  "dataSchema": {
    "name": "Product",
    "displayName": "Prodotto",
    "description": "Schema per dati prodotto",
    "fields": [
      {
        "name": "id",
        "displayName": "ID Prodotto",
        "type": "int",
        "isRequired": true,
        "validationRules": ["min:1"]
      },
      {
        "name": "name",
        "displayName": "Nome Prodotto",
        "type": "String",
        "isRequired": true,
        "validationRules": ["minLength:2", "maxLength:100"]
      },
      {
        "name": "description",
        "displayName": "Descrizione",
        "type": "String",
        "isRequired": false,
        "validationRules": ["maxLength:500"]
      },
      {
        "name": "price",
        "displayName": "Prezzo",
        "type": "double",
        "isRequired": true,
        "defaultValue": 0.0,
        "validationRules": ["min:0"]
      },
      {
        "name": "category",
        "displayName": "Categoria",
        "type": "String",
        "isRequired": false,
        "validationRules": ["maxLength:50"]
      },
      {
        "name": "sku",
        "displayName": "Codice SKU",
        "type": "String",
        "isRequired": true,
        "validationRules": ["minLength:3", "maxLength:20", "pattern:^[A-Z0-9-]+$"]
      },
      {
        "name": "inStock",
        "displayName": "Disponibile",
        "type": "bool",
        "isRequired": false,
        "defaultValue": true
      },
      {
        "name": "createdAt",
        "displayName": "Data Creazione",
        "type": "DateTime",
        "isRequired": true
      }
    ],
